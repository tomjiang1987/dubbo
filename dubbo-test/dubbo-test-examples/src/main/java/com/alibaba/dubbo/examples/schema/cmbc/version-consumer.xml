<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:cmbc-rpc="http://code.cmbc.com/schema/cmbc-rpc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.cmbc.com/schema/cmbc-rpc http://code.cmbc.com/schema/cmbc-rpc/cmbc-rpc.xsd">

    <cmbc-rpc:application name="version-consumer"  />
    
    <cmbc-rpc:registry address="zookeeper://127.0.0.1:2181"/>
    
	<cmbc-rpc:reference id="versionService" interface="com.alibaba.dubbo.examples.version.api.VersionService" version="*"/>

</beans>